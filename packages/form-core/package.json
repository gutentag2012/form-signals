{
  "name": "@signal-forms/form-core",
  "version": "0.0.1",
  "description": "Core package for Signal Form including the form state and validation logic as well as the api",
  "author": "joshua.gawenda",
  "license": "todo",
  "repository": "todo",
  "homepage": "todo",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.js",
        "types": "./dist/esm/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": ["dist", "src"],
  "peerDependencies": {
    "@preact/signals-core": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@vitest/coverage-istanbul": "^1.3.1",
    "jsdom": "^24.0.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "keywords": ["signals", "form"],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "bench": "vitest bench --typecheck false",
    "prepublishOnly": "pnpm build"
  }
}
