{
  "name": "@formsignals/validation-adapter-zod",
  "version": "0.0.9",
  "description": "Validation adapter for zod enabling the usage of zod schemas with signal forms",
  "author": "joshua.gawenda",
  "license": "MIT",
  "repository": "gutentag2012/form-signals",
  "homepage": "https://gutentag2012.github.io/form-signals/",
  "bugs": {
    "url": "https://github.com/gutentag2012/form-signals/issues"
  },
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.js",
        "types": "./dist/esm/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "dependencies": {
    "@formsignals/form-core": "0.0.9"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  },
  "keywords": [
    "signals",
    "form",
    "zod",
    "validation"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  }
}
